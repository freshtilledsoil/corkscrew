{% set project =

  {
    clientName: 'Fresh Tilled Soil',
    logoPath: 'corkscrew/assets/images/logo.jpg',
    repositoryUrl: 'http://github.com/freshtilledsoil/corkscrew',
    css: ['#', '#'],
    headJS: ['#', '#'],
    footJS: ['#', '#'],
    htmlViewer: true,
    showTemplates: true
  }

%}{% set colors = [

  {
    hex: "#fff",
    label: "White"
  },
  {
    hex: "#f7f7f7",
    label: "Lightest Gray"
  },
  {
    hex: "#eeeeee",
    label: "Lighter Gray"
  },
  {
    hex: "#c3bdb9",
    label: "Light gray"
  },
  {
    hex: "#999999",
    label: "Gray"
  },
  {  hex: "#363E40",
    label: "Dark Gray"
  },
  {
    hex: "#6ED8CE",
    label: "Teal"
  },
  {
    hex: "#FF2A62",
    label: "Pink"
  },
  {
    hex: "#81005C",
    label: "Violet"
  },
  {
    hex: "#565664",
    label: "Purple"
  },
  {
    hex: "#7A9398",
    label: "Blue gray"
  },
  {
    hex: "#2d424d",
    label: "Dark Blue"
  }

]%}{% set templates = [

  {
    name: "Home Page",
    url: "http://freshtilledsoil.com"
  },
  {
    name: "Blog",
    url: "http://freshtilledsoil.com/blog"
  }

]%}<!DOCTYPE html>
<html lang="en-us" dir="ltr" class="no-js">
<head>
{% spaceless %}
  <meta charset="utf-8">
  <title>{% block sectionName %}Overview{% endblock %} &middot; {{ project.clientName }} | Style Guide</title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta http-equiv="cleartype" content="on">

  <link rel="dns-prefetch" href="//ajax.googleapis.com">
  <link rel="dns-prefetch" href="//fonts.googleapis.com">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'overview' }) }}">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'base' }) }}">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'components' }) }}">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'modules' }) }}">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'helpers' }) }}">
  <link rel="prefetch" href="{{ path_for('navigation', { 'name': 'templates' }) }}">

  <link rel="icon" type="image/png" href="corkscrew/assets/images/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="corkscrew/assets/images/favicon-16x16.png" sizes="16x16">
  <!--[if IE]>
    <link rel="shortcut icon" href="corkscrew/assets/images/browser-icons/favicon.ico">
  <![endif]-->

  <script>

    WebFontConfig = {
      google: { families: [ 'Open+Sans:400,700:latin' ] }
    };

    (function() {
      var wf = document.createElement('script');
      wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
      '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
      wf.type = 'text/javascript';
      wf.async = 'true';
      var s = document.getElementsByTagName('script')[0];
      s.parentNode.insertBefore(wf, s);
    })();

    // remove no-js and add 'js' to the HTML
    document.documentElement.className = document.documentElement.className.replace('no-js', " ");
    document.documentElement.className += ' js ';
  </script>

  <noscript>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">
  </noscript>

  <!-- Corkscrew Core CSS -->

  <style>
  {% include 'assets/css/corkscrew.css' %}
  {% include 'assets/css/corkscrew-extend.css' %}
  </style>

  <!-- CSS for {{ project.clientName }} -->
  {% for style in project.css %}
    <link rel="stylesheet" href="{{ style }}">
  {% endfor %}

  <!-- Head JavaScript for {{ project.clientName }} -->
  {% for script in project.headJS %}
    <script src="{{ script }}"></script>
  {% endfor %}

</head>

<body class="cs-body">

  <header role="banner" id="cs-banner" class="cs-banner">
    <a href="#cs-main" class="cs-skip-link">
      Skip to content
    </a>
    <div class="cs-container">
      <nav class="cs-gutter cs-clearfix">

        <div class="cs-nav-header">
          <div class="cs-branding">
            <a href="{{ path_for('overview') }}">
              <img id="cs-logo-header" class="cs-logo" src="{{ project.logoPath }}" alt="">
              <span class="cs-is-hidden">
                Style guide for {{ project.clientName }}
              </span>
            </a>
          </div><!--/.cs-branding-->

          <div class="cs-is-menu-show">
            <a href="#cs-site-nav"
              id="cs-nav-menu-toggle"
              aria-controls="cs-site-nav">
              Menu
            </a>
          </div>
        </div><!--/.cs-nav-header-->

        <ul id="cs-site-nav" class="cs-is-clean cs-fts-nav">
          {% include '_corkscrew-navigation.twig' %}
          <!-- populate templates here -->
          <li class="{% if section == "templates" %}is-active{% endif %}">
            <a href="{{ path_for('navigation', { 'name': 'templates' }) }}">
              Templates
            </a>
            {% if project.showTemplates == true %}
              <ul id="cs-templates" class="cs-site-nav-menu">
              {% for template in templates %}
                <li><a href="{{ template.url }}">{{ template.name }}</a></li>
              {% endfor %}
              </ul>
            {% endif %}
          </li>
        </ul>

      </nav><!--/.cs-gutter-->
    </div><!--/.cs-container-->
  </header>


  <main role="main" id="cs-main" class="cs-main" aria-label="Content">

    <div class="cs-container cs-align-normal">

    {% if project.repositoryUrl %}
      <div class="cs-msg">
        <p>
          <a href="{{ project.repositoryUrl }}">
            View the repository for this project
          </a>
        </p>
      </div><!--/.cs-msg-->
    {% endif %}

      <div class="cs-gutter">
{% endspaceless %}

      {% block content %}
      {% endblock %}

{% spaceless %}
      </div><!--/.cs-gutter-->
    </div><!--/.cs-container-->
  </main><!--/.cs-main-->

  <script>
    {% include 'assets/js/corkscrew.js' %}
    {% include 'assets/js/corkscrew-extend.js' %}
  </script>

  <!-- Footer JavaScript for {{ project.clientName }} -->
  {% for script in project.footJS %}
    <script src="{{ script }}"></script>
  {% endfor %}

{% endspaceless %}

</body>
</html>
